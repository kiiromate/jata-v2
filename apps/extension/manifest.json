{
  "manifest_version": 3,
  "name": "JATA",
  "version": "1.0.0",
  "description": "JATA Chrome Extension",
  "permissions": [
    "storage",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "src/background.js"
  },
  "action": {
    "default_popup": "index.html"
  }
}
